#include <Arduino.h>

void activate()
{
    interrupts();
    attachInterrupt(digitalPintoInterrupt(3),PS_ISR,CHANGE);
}

ISR (PS_ISR)
{
  int PS = digitalRead(3);

  Serial.write(PS);
}

